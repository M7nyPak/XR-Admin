	TextLabel_2.Text = "Starting tase"
	TextLabel_2.Text = "Player Tased"
	
	game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{

Text = ("[if tase does not work then the guards team is full]");

Color =  Color3.fromHSV(math.random(100)/100,1,1);

FontSize = Enum.FontSize.Size24;

	})
	
	for i,v in pairs(GetPlayer(Player.Text)) do
		Player.Text = game.Players[v].Name
		workspace.Remote.TeamEvent:FireServer("Bright blue")
		local saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	game.Players.LocalPlayer.Character.Humanoid.Health = 0
	local A_1 = "LocalPlayer"
	
	local Event = game:GetService("Workspace").Remote.loadchar
Event:InvokeServer(A_1)
 
	

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
wait(0.9)
	local A_1 = 
{
	[1] = 
{
	["RayObject"] = Ray.new(Vector3.new(829.838562, 101.489998, 2331.25635), Vector3.new(-30.6540909, -5.42795324, 95.0308533)), 
