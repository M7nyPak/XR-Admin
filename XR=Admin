songs.Parent = game.Workspace
game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == prefix.."mv1") then
	game.StarterGui:SetCore("SendNotification", {
Title = "Music";
Text = "Music set to 1, "..ip.Name.."!";
Icon = "rbxassetid://1512959501";
Duration = 5;
})
		audio.Volume = 1
		songs.Volume = 1
		songs2.Volume = 1
	end
end)

game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == prefix.."mv2") then
	game.StarterGui:SetCore("SendNotification", {
Title = "Music";
Text = "Music set to 2, "..ip.Name.."!";
Icon = "rbxassetid://1512959501";
Duration = 5;
})
	audio.Volume = 2
	songs.Volume = 2
	songs2.Volume = 2
	
	end
end)
