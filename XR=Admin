purpless.MouseButton1Down:connect(function()
	local saveds = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
local sin = 
{
game:GetService("Players").LocalPlayer,
"Magenta"
}
game:GetService("Workspace").Remote.loadchar:InvokeServer(unpack(sin))
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saveds
end)

