game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == prefix.."fastrs") then
	fastrssa = true
	game.StarterGui:SetCore("SendNotification", {
Title = "Fast Respawn";
Text = "Fast Respawn Activated "..ip.Name.."!";
Icon = "rbxassetid://1512959501";
Duration = 5;
	})
	
	-- prison life god mode by bloxploits
while fastrssa == true do 
	wait(.1)
	if game.Players.LocalPlayer.Character.Humanoid.Health < 1 then 
		local lastPos = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").position
		local plr = game.Players.LocalPlayer.Name
		local gayevent = game:GetService("Workspace").Remote.loadchar
		gayevent:InvokeServer(plr)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(lastPos)

	end
end
end
end)
ScrollingFrame.Active = true
game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == prefix.."breaks") then
	fastrssa = false
	end
end)






local UserInputService = game:GetService("UserInputService")
UserInputService.InputBegan:connect(function(key)
	if key.KeyCode == Enum.KeyCode.Return then
		if CMDBOX.Text == "ctrl" then
	local Plr = game:GetService("Players").LocalPlayer
local Mouse = Plr:GetMouse()

Mouse.Button1Down:connect(function()
if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then return end
if not Mouse.Target then return end
Mouse.Target:Destroy()

end)
end
	end
end)
wait(.2)
CMDBOX.Text = ""
local UserInputService = game:GetService("UserInputService")
UserInputService.InputBegan:connect(function(key)
	if key.KeyCode == Enum.KeyCode.Return then
		if CMDBOX.Text == "arrestall" then
	local Player = game.Players.LocalPlayer
local cpos = Player.Character.HumanoidRootPart.CFrame
