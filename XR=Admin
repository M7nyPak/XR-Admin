game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == "/redtm") then
	
	local You = "LoEDevSSSSS" local head = workspace[You].Head:Clone() local model = Instance.new("Model",workspace) local humanoid = Instance.new("Humanoid",model) head.Parent = model model.Name = You.." LoEDevSSSSS" humanoid.MaxHealth = 0 local w = Instance.new("Weld",model) w.Part0,w.Part1 = workspace[You].Head,head
	end
end)


game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == "/nobright") then
		
game.Lighting.Brightness = 1
game.Lighting.GlobalShadows = true
	end
end)

game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == prefix.."carspin") then
	local	power = 400
local active = false
local val = Instance.new("IntValue")
val.Name = "Number"
val.Parent = game.Players.LocalPlayer
val.Value = 5
game:GetService('RunService').Stepped:connect(function()

end)
wait(.1)
local bambam = Instance.new("BodyThrust")
bambam.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
bambam.Force = Vector3.new(power,0,power)
bambam.Location = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == prefix.."bal") then
	game.Players.LocalPlayer.Character.Head.CanCollide = true
game.Players.LocalPlayer.Character.Torso.CanCollide = true
game.Players.LocalPlayer.Character["Left Leg"].CanCollide = true
game.Players.LocalPlayer.Character["Right Leg"].CanCollide = true
	local active = false
game.Players.LocalPlayer.Character.HumanoidRootPart.BodyThrust:Remove()
end
end)
	end
end)



	


game.Players.ChildAdded:connect(function(player)
    if not pcall(function()
        game.StarterGui:SetCore("SendNotification", {
Title = " Joined Game ";
Text = player.Name.. " has joined the game";
Icon = "rbxassetid://2911715226";
Duration = 8;
})                 
    end) then
        print("Error")
    end
end)
 
game.Players.ChildRemoved:connect(function(player)
    if not pcall (function()
       game.StarterGui:SetCore("SendNotification", {
Title = "Left Game ";
Text = player.Name.. " has left the game";
Icon = "rbxassetid://2911715226";
Duration = 8;
})
    end) then
        print("Error")
    end
end)




game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == "/ifdied") then
	for i,v in pairs(GetPlayer(Player.Text)) do
		
		if game.Players[v].Character.Humanoid.Health < 1 then
			game.StarterGui:SetCore("SendNotification", {
Title = "Other player";
Text = "Has Exploded";
Icon = "rbxassetid://4543990662";
Duration = 3;
})
		
		end
		
	end
	end
end)

