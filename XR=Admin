local sound3 = 4104372602
game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == prefix.."mv5") then
	game.StarterGui:SetCore("SendNotification", {
Title = "Music";
Text = "Music set to 5 Max Volume, "..ip.Name.."!";
Icon = "rbxassetid://1512959501";
Duration = 5;
})
	audio.Volume = 5
	songs.Volume = 5
	songs2.Volume = 5
	end
end)


game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == prefix.."wind") then
	audio.Volume = 0
	songs.SoundId = "rbxassetid://"..sound3
	songs:Play()
	songs.Volume = 4
	songs.Ended:wait()
	wait(10)
	audio.Volume = 3
end
end)

game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == prefix.."daft") then
	audio.Volume = 0
	songs.SoundId = "rbxassetid://"..sound3
	songs:Play()
	songs.Volume = 4
	songs.Ended:wait()
	wait(7)
	audio.Volume = 3
	
	end
end)

local sound2 = 313905409
local sound1 = 411572536
audio.Parent = game.Workspace
while true do
	wait()
	audio.SoundId = "rbxassetid://"..sound1
	audio:Play()
	
	audio.Ended:wait()
	
	
	songs.SoundId = "rbxassetid://"..sound2
	songs:Play()
	
	songs.Ended:wait()
	
	
end
