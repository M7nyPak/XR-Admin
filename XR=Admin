game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
	if (string.lower(msg) == "/crimaura")then
	crimaurass = true
	while crimaurass == true do
		
	wait(.1)
	local crimsss = game.Workspace["Criminals Spawn"].SpawnLocation -- makes local player crim
crimsss.Transparency = 0
crimsss.CanCollide = false
game.Workspace["Criminals Spawn"].SpawnLocation.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
crimsss.Transparency = 1
	for i, plr in pairs(game.Players:GetChildren()) do
		for a,w in pairs(game.Teams.Criminals:GetPlayers()) do
if w.Name == plr then
	if game.Players.LocalPlayer.Name ~= game.Players.LocalPlayer.Name then
	print("crim already")
	else
