Arrest.MouseButton1Down:connect(function()
	TextLabel_2.Text = "Arrested Player"
looparrests = true
	for i,v in pairs(GetPlayer(Player.Text)) do
		Player.Text = game.Players[v].Name
		while true do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[v].Character.HumanoidRootPart.CFrame
workspace.Remote.arrest:InvokeServer(game.Players[v].Character.Torso)
local A_1 = "LocalPlayer"
		wait(0.7)
	break
	
		end
		TextLabel_2.Text = "Spectates Gui v2.1"
		end
end)


looparrest.MouseButton1Down:connect(function()
	TextLabel_2.Text = "Arresting Player..."
	wait(0.2)
	local saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	looparrests = true
	for i,v in pairs(GetPlayer(Player.Text)) do
		Player.Text = game.Players[v].Name
		while looparrests == true do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[v].Character.HumanoidRootPart.CFrame
workspace.Remote.arrest:InvokeServer(game.Players[v].Character.Torso)
		end
		end
end)
local saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
local Event = game:GetService("Workspace").Remote.loadchar
unlooparrest.MouseButton1Down:connect(function()
	looparrests = false
local A_1 = "LocalPlayer"
	
	Event:InvokeServer(A_1)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	TextLabel_2.Text = "Spectates Gui v2.1"
end)


